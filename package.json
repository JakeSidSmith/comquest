{
  "name": "comquest",
  "version": "0.0.0",
  "description": "Composable requests with redux thunk and axios",
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "scripts": {
    "dist": "./scripts/dist",
    "start": "budo examples/ts/index.tsx:js/index.js --dir ./examples --live -- --plugin [tsify --project './tsconfig.budo.json']",
    "lint-js": "tslint --project './tsconfig.json' './{src,tests,examples}/**/*.{ts,tsx}'",
    "tests": "jest",
    "test": "npm run lint-js && npm run tests -- --runInBand --coverage",
    "prepublishOnly": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakesidsmith/comquest.git"
  },
  "keywords": [
    "composable",
    "requests",
    "reducers",
    "redux",
    "thunk",
    "axios"
  ],
  "author": "Jake 'Sid' Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakesidsmith/comquest/issues"
  },
  "homepage": "https://github.com/jakesidsmith/comquest#readme",
  "dependencies": {
    "@types/deepmerge": "^2.1.0",
    "axios": ">= 0",
    "deepmerge": "^2.1.1",
    "path-to-regexp": "^2.2.1",
    "redux": ">= 3",
    "redux-thunk": ">= 1",
    "typescript": ">= 2.9.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.8",
    "@types/react-dom": "^16.0.7",
    "budo": "^11.3.2",
    "jest": "^23.4.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "ts-jest": "^23.1.3",
    "tsify": "^4.0.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-react": "^3.6.0"
  },
  "peerDependencies": {
    "axios": ">= 0",
    "redux": ">= 3",
    "redux-thunk": ">= 1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/**/*.(js|jsx|ts|tsx)"
    ],
    "setupFiles": [
      "<rootDir>/tests/helpers/setup.ts"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(js|jsx|ts|tsx)$",
    "testPathIgnorePatterns": [
      "<rootDir>/tests/helpers/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "testURL": "http://localhost"
  }
}
